ErrorDocument 404 http://localhost/justcare/404.php

Redirect /justcare/login /justcare/login.php
Redirect /signup /justcare/login.php
Redirect /justcare/dashboard /justcare/dashboard.php
Redirect /justcare/mydashboard /justcare/dashboard.php
Redirect /justcare/verifyTransaction.php/:reference /justcare/verifyTransaction.php?reference=:ref&id=:id&name=:name&email=:email
Redirect justcare/login /justcare/login.php

RewriteEngine On
RewriteBase /justcare/
RewriteRule ^index\\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /justcare/index.php [L]




